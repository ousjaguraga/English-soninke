
{
  "builds": [
    {
      "src": "Soninke/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "buildCommand": "bash setup.sh",
         "distDir": "staticfiles"
      }
    }
  ],
 "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Soninke/wsgi.py"
    }
  ]
}
